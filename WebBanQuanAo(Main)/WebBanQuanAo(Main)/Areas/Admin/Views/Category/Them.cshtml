@model WebBanQuanAo_Main_.Models.Category

@{
    ViewBag.Title = "Them";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Form thêm mới danh mục</h2>
@using (Html.BeginForm("Them", "Category", FormMethod.Post))
{
    @Html.AntiForgeryToken();
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(m => m.IDCate)
            </th>
            <td>
                @Html.EditorFor(m => m.IDCate)
            </td>


        </tr>
        <tr>
            <th>
                @Html.DisplayNameFor(m => m.NameCate)
            </th>
            <td>
                @Html.EditorFor(m => m.NameCate)
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" value="Lưu" />
            </td>
        </tr>
    </table>
}
@Html.ActionLink("Quay về danh sách", "DanhSachDanhMuc", null, new { @class = "btn btn-success" })

